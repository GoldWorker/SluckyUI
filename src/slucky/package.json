{
    "name": "slucky",
    "version": "0.4.3",
    "description": "SluckyUI component base on react,but style you can use on other platform (Vue/Angular/...).",
    "main": "lib/index.js",
    "homepage": "http://www.brandf.cn",
    "repository": {
        "type": "git",
        "url": "https://github.com/GoldWorker/SluckyUI/tree/master/src/slucky"
    },
    "scripts": {
        "test": "jest --coverage",
        "test:dev": "jest --watch",
        "build_index": "./node_modules/.bin/babel ./index.jsx --out-dir ./",
        "build_src": "./node_modules/.bin/babel ./src --out-dir ./lib",
        "build": "npm run build_index && npm run build_src",
        "pub": "npm run build && npm publish",
        "testCommand": "webpack --mode production",
        "publishByWebpack": "webpack --mode production && npm publish"
    },
    "keywords": [
        "react",
        "ui",
        "component",
        "slucky",
        "sluckyUI"
    ],
